{
    "diagnostic.errorSign": "✘",
    "diagnostic.warningSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.hintSign":"ﯦ",
    "diagnostic.checkCurrentLine": true,
    "diagnostic.messageTarget": "echo",
    "python.analysis.typeCheckingMode": "off",
    "coc.preferences.useQuickfixForLocations": true,
    "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "typescriptreact", "typescript"],
    "suggest.completionItemKindLabels": {
        "function": "",
        "method": "ƒ",
        "variable": "",
        "constant": "",
        "struct": "",
        "class": "",
        "interface": "ﰮ",
        "text": "",
        "enum": "了",
        "enumMember": "",
        "module": "",
        "color": "",
        "property": "",
        "field": "\uf93d",
        "unit": "",
        "file": "",
        "value": "",
        "event": "\ufacd",
        "folder": "",
        "keyword": "",
        "snippet": "﬌",
        "operator": "\uf915",
        "reference": "\uf87a",
        "typeParameter": "\uf278",
        "default": "\uf29c"
    },
    "rust-analyzer.server.path": "/usr/local/bin/rust-analyzer",
    "omnisharp.version": "v1.37.3",
    "languageserver": {
        "lua": {
            "command": "~/.local/share/nvim/lspinstall/lua/./sumneko-lua-language-server",
            "filetypes": ["lua"],
            "rootPatterns": [".git/"],
            "settings": {
                "Lua": {
                    "telemetry": {
                        "enable": true
                    },
                    "workspace": {
                        "library": {
                            "/usr/local/share/nvim/runtime/lua": true,
                            "/usr/local/share/nvim/runtime/lua/vim/lsp": true
                        },
                        "maxPreload": 2000,
                        "preloadFileSize": 1000
                    },
                    "runtime": {
                        "version": "LuaJIT"
                    },
                    "diagnostics": {
                        "enable": true,
                        "globals": ["hs", "vim", "it", "describe", "before_each", "after_each"],
                        "disable": ["lowercase-global"]
                    },
                    "completion": {
                        "keywordSnippet": "Disable"
                    }
                }
            }
        }
    }
}
